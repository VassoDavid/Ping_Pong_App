public class Doubles extends Match{
    Player RedPlayerOne;
    Player RedPlayerTwo;
    Player BluePlayerOne;
    Player BluePlayerTwo;
    public Doubles(Player[] Players){
        super(Players);
        this.RedPlayerOne = this.Players[0];
        this.RedPlayerTwo = this.Players[1];
        this.BluePlayerOne = this.Players[2];
        this.BluePlayerTwo = this.Players[3];
    }
    public void DefineDoubleMatch(Player[] Winners, Player[] Losers)
    {
        this.DefineMatch(Winners,Losers);
    }
}
