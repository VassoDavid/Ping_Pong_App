public class Singles extends Match
{
    static Player PlayerOne; 
    static Player PlayerTwo;
    public Singles(Player[] Players)
    {
        super(Players);
        this.PlayerOne = this.Players[0];
        this.PlayerTwo = this.Players[1];
    }
    public static void DefineSinglesMatch(Player SinglesWinner,Player SinglesLoser){
        Player[] Winner = {SinglesWinner};
        Player[] Loser = {SinglesLoser};
        DefineMatch(Winner,Loser);
    }
}
